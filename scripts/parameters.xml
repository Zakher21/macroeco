<?xml version='1.0'?>
<METE>
  <analysis scriptname='skeleton' version='0.5' interactive='T'>
    <run name='red'>
        <param name='color' value='red' />
    </run>
    <run name='black'>
        <param name='color' value='black' />
    </run>
  </analysis>
  <analysis scriptname='callparams' version='0.5' interactive='F'>
    <run name='autoname0'>
      <param name='size' value='4' />
      <param name='species' value='foo' />
      <param name='layers' value='[1, 5, 13]' />
    </run>
  </analysis>
  <analysis scriptname='analyze_my_sad' version='0.5' interactive='F'>
    <run name='METE_anal'>
        <param name='grid' value='[(1,1)]'/>
        <param name='distr' value='mete'/>
        <param name='title' value='Observed vs. Predicted cdf (METE)'/>
    </run>
    <run name='Lognormal_anal'>
        <param name='grid' value='[(1,1)]'/>
        <param name='distr' value='trun_plognorm'/>
        <param name='title' value='Observed vs. Predicted cdf (trun_lognormal)'/>
    </run>
  </analysis>
  <analysis scriptname='analyze_commonality' version='0.5' interactive='F'>
    <run name='commonality'>
        <param name='grid' value='[(20,20), (10,10)]'/>
    </run>
  </analysis>
  <analysis scriptname='analyze_values' version='0.5' interactive='F'>
    <run name='summary'>
        <param name='distr' value="['mete', 'neg_binom', 'geo']"/>
        <param name='grid' value='[(1,1), (2,2)]'/>
    </run>
  </analysis>
  <analysis scriptname='analyze_my_sar' version='0.5' interactive='F'>
    <run name='obs_pred_sar'>
        <param name='gridded' value='True'/>
        <param name='samples' value='25'/>
        <param name='grid' value='[(1,1), (2,2), (4,4), (8,8), (16,16)]'/>
    </run>
  </analysis>
</METE>
